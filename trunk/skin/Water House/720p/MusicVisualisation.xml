<window id="2006">
	<defaultcontrol>2</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<controls>
		<control type="visualisation" id="2">
			<description>visualisation</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
		</control>
		<control type="progress" id="23">
			<description>Progressbar</description>
			<posx>140</posx>
			<posy>640</posy>
			<width>1000</width>
			<height>10</height>
			<info>Player.Progress</info>
			<texturebg>PlayBarbg.png</texturebg>
			<lefttexture>play_01.png</lefttexture>
			<midtexture>play_02.png</midtexture>
			<righttexture>play_03.png</righttexture>
			<overlaytexture>-</overlaytexture>
	<visible>Player.Paused</visible>
	<animation effect="fade" start="0" time="300">WindowOpen</animation>
	<animation effect="fade" end="0" time="300">WindowClose</animation>
		</control>

			<control type="label" id="1">
				<description>time label</description>
				<posx>122</posx>
				<posy>636</posy>
<textcolor>FFffffff</textcolor>
<visible>Player.Paused</visible>				
<label>$INFO[VideoPlayer.Time]</label>
				<align>right</align>
				<font>font5</font>
	<animation effect="fade" start="0" time="300">WindowOpen</animation>
	<animation effect="fade" end="0" time="300">WindowClose</animation>
			</control>

			<control type="label" id="1">
				<description>time label</description>
				<posx>1160</posx>
				<posy>636</posy>
<textcolor>FFffffff</textcolor>
	<visible>Player.Paused</visible>
				<label>$INFO[Player.Duration]</label>
