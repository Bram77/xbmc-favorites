<window id="56">
	<defaultcontrol>1</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>
		<control type="image">
			<description>background image</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>720</height>
			<texture>background.png</texture>
			<aspectratio>keep</aspectratio>
		</control>
<control type="grouplist" id="100">
      <posx>75</posx>
      <posy>30</posy>
      <width>600</width>
      <height>720</height>
      <itemgap>1</itemgap>
      <orientation>vertical</orientation>
	<onup>100</onup>
	<ondown>100</ondown>
<control type="button" id="1">
      <posx>80</posx>
      <posy>60</posy>
      <width>250</width>
      <height>40</height>
      <label>set pictures custom background folder</label>
      <font>small</font>
      <textcolor>normal</textcolor>
      <disabledcolor>fadetext</disabledcolor>
	<focusedcolor>selected</focusedcolor>
      <align>left</align>
      <onclick>Skin.SetPath(pictures)</onclick>
</control>
