<window id="115">
	<defaultcontrol>901</defaultcontrol>
	<include>dialogfade</include>
	<visible>!Window.IsVisible(2901) + [player.seeking | player.displayafterSeek | player.paused | player.forwarding | player.rewinding]</visible>
	<controls>
		<control type="image">
		<posy>310</posy>
		<texture>videoosdcorner.png</texture>
		<visible>VideoPlayer.IsFullscreen</visible>
		</control>
		<control type="group" id="101">
		<posy>145</posy>
		<control type="group">
			<posx>195</posx>
			<posy>547</posy>
		<control type="label" id="0">
			<label>paused</label>
			<align>center</align>
			<visible>player.paused</visible>
		</control>
		<control type="label" id="0">
			<label>ff2x</label>
			<align>center</align>
			<visible>player.forwarding2x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>ff4x</label>
			<visible>player.forwarding4x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>ff8x</label>
			<visible>player.forwarding8x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>ff16x</label>
			<visible>player.forwarding16x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>ff32x</label>
			<visible>player.forwarding32x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>rw2x</label>
			<visible>player.rewinding2x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>rw4x</label>
			<visible>player.rewinding4x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>rw8x</label>
			<visible>player.rewinding8x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>rw16x</label>
			<visible>player.rewinding16x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>rw32x</label>
			<visible>player.rewinding32x</visible>
		</control>
	</control>
		<control type="progress" id="23">
			<description>progressbar</description>
			<posx>50</posx>
			<posy>532</posy>
			<width>300</width>
			<height>12</height>
			<info>player.progress</info>
			<visible>true</visible>
		</control>
	<control type="group">
		<description>Now Playing Info For Fullscreen</description>
		<visible>VideoPlayer.IsFullscreen</visible>
		<control type="label" id="0">
			<description>movie info</description>
			<width>400</width>
			<posx>50</posx>
			<posy>512</posy>
			<font>info</font>
			<visible>VideoPlayer.Content(movies)</visible>
			<textcolor>normal</textcolor>
			<align>left</align>
			<label>[LOWERCASE]$INFO[VideoPlayer.Title][/LOWERCASE]</label>
			<scroll>false</scroll>
		</control>
		<control type="label" id="0">
			<description>episode_info</description>
			<width>400</width>
			<posx>50</posx>
			<posy>488</posy>
			<font>info</font>
			<visible>VideoPlayer.Content(episodes)</visible>
			<textcolor>selected</textcolor>
			<align>left</align>
			<label>[LOWERCASE]$INFO[VideoPlayer.TVShowTitle] s[B]$INFO[VideoPlayer.Season][/B]e[B]$INFO[VideoPlayer.Episode][/B][CR]$INFO[VideoPlayer.Title][/LOWERCASE]</label>
			<scroll>false</scroll>
		</control>
		<control type="image" id="0">
			<description>Playing Video Art</description>
			<posx>50</posx>
			<posy>85</posy>
			<width>300</width>
			<height>400</height>
			<texture fallback="">$INFO[videoplayer.cover]</texture>
			<aspectratio aligny="bottom" align="left">keep</aspectratio>
			<visible>VideoPlayer.Content(episodes)</visible>
		</control>
		<control type="image" id="0">
			<description>Playing Video Art</description>
			<posx>50</posx>
			<posy>65</posy>
			<width>300</width>
			<height>446</height>
			<texture fallback="">$INFO[videoplayer.cover]</texture>
			<aspectratio aligny="bottom" align="left">keep</aspectratio>
			<visible>VideoPlayer.Content(movies)</visible>
		</control>
	</control>
		<control type="label">
			<posx>350</posx>
			<posy>547</posy>
			<height>40</height>
			<description>duration</description>
			<textcolor>normal</textcolor>
			<width>75</width>
			<label>$INFO[VideoPlayer.Duration]</label>
			<align>right</align>
		</control>
		<control type="label">
			<posx>50</posx>
			<posy>547</posy>
			<height>40</height>
			<textcolor>normal</textcolor>
			<description>Time</description>
			<width>75</width>
			<label>$INFO[VideoPlayer.Time]</label>
			<align>left</align>
		</control>

	</control>
	</controls>
	</window>