<window id="125">
	<defaultcontrol always="true">1</defaultcontrol>
	<include>dialogfade</include>
	<controls>
		<control type="label">
			<description>bookmarks_label</description>
			<font>title</font>
			<textcolor>white</textcolor>
			<shadowcolor>selected</shadowcolor>
			<posx>375</posx>
			<posy>394</posy>
			<label>298</label>
		</control>
		<control type="grouplist" id="1">
			<posx>375</posx>
			<posy>410</posy>
			<height>45</height>
			<itemgap>5</itemgap>
			<orientation>horizontal</orientation>
			<ondown>11</ondown>
			<control type="button" id="2">
				<description>create</description>
				<align>left</align>
				<width>175</width>
				<label>create bookmark</label>
				<font>small</font>
				<textcolor>normal</textcolor>
				<focuscolor>selected</focuscolor>
				<onup>Dialog.Close(125)</onup>
			</control>
			<control type="button" id="3">
				<description>clear</description>
				<width>175</width>
				<align>left</align>
				<label>clear bookmarks</label>
				<font>small</font>
				<textcolor>normal</textcolor>
				<focuscolor>selected</focuscolor>
				<onup>Dialog.Close(125)</onup>
			</control>
		</control>
		<control type="fixedlist" id="11">
			<posx>375</posx>
			<posy>445</posy>
			<width>905</width>
			<height>300</height>
			<onleft>-</onleft>
			<onright>-</onright>
			<ondown>Dialog.Close(125)</ondown>
			<onup>2</onup>
			<scrolltime>200</scrolltime>
			<viewtype label="536">icon</viewtype>
     			<focusposition>0</focusposition>
			<orientation>horizontal</orientation>
			<itemlayout height="215" width="240">
				<control type="image">
					<posx>10</posx>
					<posy>7</posy>
					<width>220</width>
					<height>175</height>
					<aspectratio aligny="bottom">keep</aspectratio>
					<info>ListItem.Icon</info>
				</control>
			</itemlayout>
			<focusedlayout height="215" width="250">
				<control type="image">
					<posx>0</posx>
					<posy>22</posy>
					<width>240</width>
					<height>190</height>
					<texture>media-background-black.png</texture>
					<visible>control.HasFocus(11)</visible>
				</control>
				<control type="image">
					<posx>10</posx>
					<posy>7</posy>
					<width>220</width>
					<height>175</height>
					<align>center</align>
					<aspectratio aligny="bottom">keep</aspectratio>
					<info>ListItem.Icon</info>
				</control>
				<control type="label">
					<posx>230</posx>
					<posy>185</posy>
					<width>225</width>
					<height>22</height>
					<font>small</font>
					<textcolor>white</textcolor>
					<align>right</align>
					<aligny>center</aligny>
					<info>ListItem.Label</info>
					<visible>control.HasFocus(11)</visible>
				</control>
			</focusedlayout>
		</control>
	<control type="image">
		<posx>697</posx>
		<posy>466</posy>
		<width>583</width>
		<height>254</height>
		<texture>bookmarkfadeout.png</texture>
	</control>
	</controls>
</window>
