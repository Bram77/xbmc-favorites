<window>
	<id>2005</id>
	<defaultcontrol>13</defaultcontrol>
	<controls>
		<control>
			<description>background image wide</description>
			<type>image</type>
			<posx>542</posx>
			<posy>574</posy>
			<width>738</width>
			<height>118</height>
			<texture>overlayinfowide.png</texture>
		</control>

		<control>
			<description>background image flowers</description>
			<type>image</type>
			<posx>0</posx>
			<posy>363</posy>
			<width>542</width>
			<height>357</height>
			<texture>overlayinfo.png</texture>
		</control>

		<control>
			<description>DVD Image</description>
			<type>image</type>
			<posx>6</posx>
			<posy>568</posy>
			<width>91</width>
			<height>130</height>
			<info>VideoPlayer.Cover</info>
			<aspectratio>keep</aspectratio>
		</control>

		<control>
			<description>Cover Case</description>
			<type>image</type>
			<posx>3</posx>
			<posy>565</posy>
			<width>97</width>
			<height>142</height>
			<texture>dvdcoveroverlaybig.png</texture>
		</control>

		<control>
			<description>time label</description>
			<type>label</type>
			<width>430</width>
			<posx>140</posx>
			<posy>598</posy>
			<label>$INFO[VideoPlayer.Time] / $INFO[Player.Duration]</label>
			<align>left</align>
			<font>font24</font>
			<shadowcolor>60000000</shadowcolor>
		</control>

		<control>
			<description>Director label</description>
			<type>label</type>
			<width>445</width>
			<posx>140</posx>
			<posy>625</posy>
			<info>VideoPlayer.Director</info>
			<align>left</align>
			<font>font24</font>
			<shadowcolor>60000000</shadowcolor>
		</control>

		<control>
			<description>Title label</description>
			<type>label</type>
			<posx>140</posx>
			<posy>648</posy>
			<width>880</width>
			<info>VideoPlayer.Title</info>
			<align>left</align>
			<font>font24B</font>
			<shadowcolor>60000000</shadowcolor>
		</control>

		<control>
			<description>static endtime</description>
			<type>label</type>
			<width>200</width>
			<posx>1270</posx>
			<posy>625</posy>
			<label>Finish time</label>
			<align>right</align>
			<font>font24</font>
			<shadowcolor>60000000</shadowcolor>
		</control>

		<control>
			<description>When is my movie done</description>
			<type>label</type>
			<width>290</width>
			<posx>1270</posx>
			<posy>648</posy>
			<label>$INFO[Player.FinishTime]</label>
			<align>right</align>
			<font>font24B</font>
			<shadowcolor>60000000</shadowcolor>
		</control>
	</controls>

</window>

