<window>
  <id>115</id>
  <defaultcontrol>1</defaultcontrol>
  <include>dialog-effect</include>
  <visible>[Player.Seeking | Player.DisplayAfterSeek | Window.IsActive(MusicOSD) | Player.Paused | Player.Forwarding | Player.Rewinding] + !Window.IsActive(VideoOSD)</visible>
    <coordinates>
    <system>1</system>
    <origin x="939" y="42">true</origin>
  </coordinates>
  <controls>

    <!-- playstate images -->

    <control>
      <description>pause image</description>
      <type>image</type>
      <id>0</id>
      <align>right</align>
      <posx>-59</posx>
      <posy>8</posy>
      <height>60</height>
      <width>60</width>
      <visible>Player.Paused</visible>
      <texture>video-pause.png</texture>
      <colorkey>0</colorkey>
      <colordiffuse>ffffffff</colordiffuse>
	  <include>dialog-effect-slide</include>
    </control>

    <control>
      <description>2x image</description>
      <type>image</type>
      <id>0</id>
      <align>right</align>
