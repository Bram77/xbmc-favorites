    </control>

    <!-- progressbar dialog -->

    <control>
      <description>Popup Seek Dialog Background</description>
      <type>image</type>
      <id>23</id>
      <width>296</width>
      <height>75</height>
      <texture>voulume-panel-bg.png</texture>
    </control>

    <control>
      <description>Elapsed VideoTime Label</description>
      <type>label</type>
      <id>22</id>
      <posx>140</posx>
      <posy>10</posy>
      <font>font24</font>
      <align>right</align>
      <info>VideoPlayer.Time</info>
      <width>120</width>
      <height>16</height>
      <visible>!Player.Seeking + Player.HasVideo</visible>
    </control>

    <control>
      <description>Elapsed MusicTime Label</description>
      <type>label</type>
      <id>22</id>
      <posx>140</posx>
      <posy>10</posy>
      <font>font24</font>
      <align>right</align>
      <info>MusicPlayer.Time</info>
      <width>120</width>
      <height>16</height>
      <visible>!Player.Seeking + Player.HasAudio</visible>
    </control>

    <control>
      <description>Seek Time Label</description>
      <type>label</type>
      <id>22</id>
      <posx>140</posx>
      <posy>10</posy>
      <font>font24</font>
      <align>right</align>
      <info>Player.SeekTime</info>
      <width>100</width>
      <height>16</height>
      <visible>Player.Seeking</visible>
    </control>

    <control>
      <description>suffix label</description>
      <type>label</type>
      <id>22</id>
      <posx>145</posx>
      <posy>10</posy>
      <label>|</label>
      <align>left</align>
      <font>font24</font>
    </control>

    <control>
      <description>VideoDuration Label</description>
      <type>label</type>
      <id>999</id>
      <posx>155</posx>
      <posy>10</posy>
      <info>VideoPlayer.Duration</info>
      <visible>Player.HasVideo</visible>
      <font>font24</font>
      <align>left</align>
      <width>120</width>
      <height>16</height>
    </control>

    <control>
      <description>MusicDuration Label</description>
      <type>label</type>
      <id>999</id>
      <posx>155</posx>
      <posy>10</posy>
      <info>MusicPlayer.Duration</info>
      <visible>Player.HasAudio</visible>
      <font>font24</font>
      <align>left</align>
      <width>100</width>
      <height>16</height>
    </control>

    <control>
      <description>Progressbar</description>
      <type>progress</type>
      <id>23</id>
      <posx>15</posx>
      <posy>45</posy>
      <width>262</width>
      <height>5</height>
      <info>Player.Progress</info>
      <texturebg>progress_back.png</texturebg>
      <lefttexture>progress_left.png</lefttexture>
      <midtexture>progress_mid.png</midtexture>
      <righttexture>progress_right.png</righttexture>
      <overlaytexture>-</overlaytexture>
      <visible>true</visible>
    </control>

    <control>
      <description>Seek Slider</description>
      <type>slider</type>
      <id>401</id>
      <posx>10</posx>
      <posy>45</posy>
      <width>262</width>
      <height>5</height>
      <texturesliderbar>seekslider.png</texturesliderbar>
      <visible>true</visible>
      <onup>1</onup>
      <ondown>1</ondown>
      <onleft>1</onleft>
      <onright>1</onright>
    </control>

  </controls>

</window>

