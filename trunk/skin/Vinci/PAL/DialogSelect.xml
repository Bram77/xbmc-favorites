<window>
  <id>2000</id>
  <defaultcontrol>5</defaultcontrol>
  <include>dialog-effect-special</include>
  <type>dialog</type>
  <controls>

    <control>
      <description>filemanager background image 2</description>
      <type>image</type>
      <width>720</width>
      <height>576</height>
      <texture>trainerpanel_bg.png</texture>
    </control>

    <control>
      <description>background image</description>
      <type>image</type>
      <id>1</id>
      <posx>250</posx>
      <posy>70</posy>
      <width>218</width>
      <height>40</height>
      <texture>Header-SelectAlbum.png</texture>
    </control>

    <control>
      <description>number of files text label</description>
      <type>label</type>
      <id>2</id>
      <posx>135</posx>
      <posy>470</posy>
      <label>-</label>
      <font>font13</font>
      <textcolor>d0ffffff</textcolor>
    </control>

    <control>
      <description>pics listcontrol</description>
      <type>listcontrol</type>
      <id>3</id>
      <posx>100</posx>
      <posy>150</posy>
      <width>520</width>
      <height>350</height>
      <spinposx>419</spinposx>
      <spinposy>330</spinposy>
      <font>font12</font>
      <textoffsetx>10</textoffsetx>
      <aligny>center</aligny>
      <onleft>5</onleft>
      <onright>5</onright>
      <onup>5</onup>
      <ondown>5</ondown>
    </control>

    <control>
      <description>IMDB</description>
      <type>button</type>
      <id>5</id>
      <posx>100</posx>
      <posy>119</posy>
      <width>520</width>
      <font>font12</font>
      <textoffsetx>10</textoffsetx>
      <aligny>center</aligny>
      <label>368</label>
      <texturefocus>list-focus.png</texturefocus>
      <texturenofocus>-</texturenofocus>
      <onleft>3</onleft>
      <onright>3</onright>
      <onup>3</onup>
      <ondown>3</ondown>
    </control>

    <control>
      <description>background image</description>
      <type>image</type>
      <id>1</id>
      <width>720</width>
      <height>576</height>
      <texture>trainerpanel.png</texture>
    </control>

  </controls>

</window>

