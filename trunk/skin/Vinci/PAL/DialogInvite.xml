<window>
  <id>102</id>
  <defaultcontrol>30</defaultcontrol>
  <controls>

    <control>
      <description>Dialog Background</description>
      <type>image</type>
      <id>0</id>
      <posx>137</posx>
      <posy>197</posy>
      <width>410</width>
      <height>270</height>
      <texture>dialog-invite.png</texture>
    </control>

    <control>
      <description>Send Invitation Heading</description>
      <type>label</type>
      <id>1</id>
      <posx>190</posx>
      <posy>201</posy>
      <label>15054</label>
      <font>font14</font>
      <textcolor>FFFFFFFF</textcolor>
    </control>

    <control>
      <description>Choose Game</description>
      <type>label</type>
      <id>1</id>
      <posx>190</posx>
      <posy>240</posy>
      <label>15055</label>
      <font>font12</font>
      <textcolor>FF000000</textcolor>
    </control>

    <control>
      <description>Games Spin Control</description>
      <type>spincontrol</type>
      <id>30</id>
      <posx>190</posx>
      <posy>270</posy>
      <reverse>yes</reverse>
      <align>left</align>
      <font>font13</font>
      <onleft>30</onleft>
      <onright>30</onright>
      <onup>10</onup>
      <ondown>20</ondown>
      <textoffsetx>0</textoffsetx>
    </control>

    <control>
      <description>Enter Message</description>
      <type>label</type>
      <id>4</id>
      <posx>190</posx>
      <posy>300</posy>
      <label>15056</label>
      <font>font12</font>
      <textcolor>FF000000</textcolor>
    </control>

    <control>
      <description>Message Field</description>
      <type>button</type>
      <id>20</id>
      <posx>190</posx>
      <posy>330</posy>
      <width>330</width>
      <label>15060</label>
      <font>font13</font>
      <textcolor>D0FFFFFF</textcolor>
      <texturefocus>list-focus.png</texturefocus>
      <texturenofocus>list-nofocus.png</texturenofocus>
      <onleft>20</onleft>
      <onright>20</onright>
      <onup>30</onup>
      <ondown>10</ondown>
    </control>

    <control>
      <description>Send button</description>
      <type>button</type>
      <id>10</id>
      <posx>360</posx>
      <posy>385</posy>
      <width>75</width>
      <height>35</height>
      <label>15057</label>
      <align>center</align>
      <onleft>10</onleft>
      <onright>11</onright>
      <onup>20</onup>
      <ondown>30</ondown>
    </control>

    <control>
      <description>Cancel button</description>
      <type>button</type>
      <id>11</id>
      <posx>445</posx>
      <posy>385</posy>
      <width>75</width>
      <height>35</height>
      <label>222</label>
      <align>center</align>
      <onleft>10</onleft>
      <onright>11</onright>
      <onup>20</onup>
      <ondown>30</ondown>
    </control>

  </controls>

</window>

