<window>
	<id>34</id>
	<defaultcontrol>2</defaultcontrol>
	<animation effect="fade" time="800">WindowClose</animation> 
	<allowoverlay>no</allowoverlay>
	<controls>
<control>
		<description>Panel Image</description>
		<type>image</type>
		<posx>5</posx>
		<posy>10</posy>
		<width>710</width>
		<height>556</height>
		<texture>background2.png</texture>
        </control>



<control>
		<description>Background</description>
		<type>image</type>
		<posx>0</posx>
		<posy>0</posy>
		<width>720</width>
		<height>576</height>
		<texture>Back-Row.png</texture>
		<animation effect="fade" time="800">WindowOpen</animation>
	    <animation effect="fade" time="800">WindowClose</animation> 
        </control>

<control>
		<description>Background</description>
		<type>image</type>
		<posx>-10</posx>
		<posy>90</posy>
		<width>740</width>
		<height>425</height>
		<texture>black.png</texture>
		<animation effect="fade" time="800">WindowOpen</animation>
	    <animation effect="fade" time="800">WindowClose</animation> 
        </control>

<control>
		<description>Background</description>
		<type>image</type>
		<posx>-10</posx>
		<posy>85</posy>
		<width>740</width>
		<height>10</height>
		<texture>separatorosd.png</texture>
		<animation effect="fade" time="800">WindowOpen</animation>
	    <animation effect="fade" time="800">WindowClose</animation> 
        </control>


<control>
		<description>Background</description>
		<type>image</type>
		<posx>-10</posx>
		<posy>510</posy>
		<width>740</width>
		<height>10</height>
		<texture>separatorosd.png</texture>
		<animation effect="fade" time="800">WindowOpen</animation>
	    <animation effect="fade" time="800">WindowClose</animation> 
        </control>	

	<control type="radiobutton" id="4">
		<description>Enable Login Screen</description>
		<posx>10</posx>
		<posy>110</posy>
		<height>30</height>
		<width>150</width>
		<label>20096</label>
		<font>menubutton</font>
		<onup>2</onup>
		<ondown>2</ondown>
		</control>


	<control type="fadelabel" id="3">
		<description>Last Loaded</description>
		<type>fadelabel</type>
		<id>3</id>
		<posx>210</posx>
		<posy>525</posy>
		<width>656</width>
		<label>-</label>
		<font>font13</font>
		<textcolor>d0ffffff</textcolor>
		</control>

	<control type="list" id="2">
		<posx>150</posx>
		<posy>160</posy>
		<width>500</width>
		<height>370</height>
		<onleft>4</onleft>
		<onright>90</onright>
		<onup>4</onup>
		<ondown>4</ondown>
		<pagecontrol>90</pagecontrol>
		<visible>!Window.IsActive(126)</visible>
		<itemlayout height="27">
			<control type="image">
				<posx>0</posx>
				<posy>5</posy>
				<width>16</width>
				<height>16</height>
				<info>ListItem.Icon</info>
				<texture>defaultfolderbig.png</texture>
			</control>
			<control type="label">
				<posx>25</posx>
				<posy>2</posy>
				<width>400</width>
				<height>16</height>
				<font>font13</font>
				<textcolor>FFcbcbcb</textcolor>
				<info>ListItem.Label</info>
			</control>
			<control type="label">
				<posx>480</posx>
				<posy>2</posy>
				<width>150</width>
				<height>16</height>
				<font>font13</font>
				<align>right</align>
				<textcolor>90cbcbcb</textcolor>
				<include>selectedcolor</include>
				<info>ListItem.Label2</info>
			</control>
		</itemlayout>
		<focusedlayout height="40">
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>500</width>
				<height>40</height>
				<texture>list-focus.png</texture>
				<visible>Control.HasFocus(2)</visible>
			</control>
			<control type="image">
				<posx>0</posx>
				<posy>0</posy>
				<width>500</width>
				<height>40</height>
				<texture>-</texture>
				<visible>!Control.HasFocus(2)</visible>
			</control>
			<control type="image">
				<posx>5</posx>
				<posy>3</posy>
				<width>32</width>
				<height>32</height>
				<info>ListItem.Icon</info>
				<texture>defaultfolderbig.png</texture>
			</control>
			<control type="label">
				<posx>45</posx>
				<posy>8</posy>
				<width>370</width>
				<height>16</height>
				<font>font18</font>
				<textcolor>ffffffff</textcolor>
				<scroll>true</scroll>
				<info>ListItem.Label</info>
			</control>
			<control type="label">
				<posx>460</posx>
				<posy>8</posy>
				<width>250</width>
				<height>16</height>
				<font>font18</font>
				<align>right</align>
				<textcolor>90cbcbcb</textcolor>
				<include>selectedcolor</include>
				<info>ListItem.Label2</info>
			</control>
		</focusedlayout>
	</control>
	
<control type="image">
    <description>Logo</description>
    <posx>15</posx>
    <posy>10</posy>
    <width>40</width>
    <height>40</height>
    <texture>unknown-user.png</texture>
    <visible>!Skin.HasSetting(menulabels)</visible>
	<animation effect="fade" time="200">WindowOpen</animation>
	<animation effect="fade" time="200">WindowClose</animation>
	 </control>

<control type="label">
	<posx>60</posx>
	<posy>30</posy>
	<align>left</align>
	<aligny>center</aligny>
	<font>special13</font>
	<textcolor>FFffffff</textcolor>
	<visible>!Skin.HasSetting(menulabels)</visible>
	<animation effect="fade" time="200">WindowOpen</animation>
	<animation effect="fade" time="200">WindowClose</animation>
    <label>13200</label>
</control>
<include>Music-Info4:3</include>
	</controls>
</window>