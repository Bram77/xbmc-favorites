				<textureradionofocus>check-boxnf.png</textureradionofocus>
				<texturefocus border="2">list-focus.png</texturefocus>
				<texturenofocus border="2">list-nofocusD.png</texturenofocus>
				<font>menu</font>
				<textcolor>label-text</textcolor>
				<disabledcolor>disabled-text</disabledcolor>
				<textoffsetx>10</textoffsetx>
				<textoffsety>4</textoffsety>
			</control>
			
			<control type="spincontrolex" id="9">
				<description>Default SpinControlex</description>
				<width>300</width>
				<font>menu</font>
				<height>33</height>
				<textcolor>button-text</textcolor>
				<disabledcolor>disabled-text</disabledcolor>
				<texturefocus border="2">list-focus.png</texturefocus>
				<texturenofocus border="2">list-nofocusD.png</texturenofocus>
				<spinwidth>16</spinwidth>
				<spinheight>16</spinheight>
				<reverse>yes</reverse>
				<textoffsetx>10</textoffsetx>
				<textoffsety>4</textoffsety>
			</control>
			
