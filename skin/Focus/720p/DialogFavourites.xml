<window type="dialog" id="105">
	<defaultcontrol always="true">450</defaultcontrol>
	
	<include>dialog-open-fav</include>

	<coordinates>
		<system>1</system>
		<posx>510</posx>
		<posy>135</posy>
	</coordinates>

	<controls>

		<control type="image">
			<posx>-27</posx>
			<posy>-26</posy>
			<width>300</width>
			<height>365</height>
			<texture border="38">dialogpanel.png</texture>
		</control>

		<control type="group">
			<control type="list" id="450">
			<posx>0</posx>
			<posy>0</posy>
			<width>246</width>
			<height>315</height>
			<onleft>1</onleft>
			<onright>10</onright>
			<onup>450</onup>
			<ondown>450</ondown>
			<scrolltime>200</scrolltime>
			
			<itemlayout height="35">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>246</width>
					<height>33</height>
					<texture border="2">list-nofocusD.png</texture>
				</control>
				<control type="label">
					<posx>10</posx>
					<posy>0</posy>
					<width>236</width>
					<height>33</height>
					<font>h2D</font>
					<align>left</align>
					<aligny>center</aligny>
					<info>ListItem.Label</info>
				</control>
			</itemlayout>
			
			<focusedlayout height="35">
				<control type="image">
					<posx>0</posx>
					<posy>0</posy>
					<width>246</width>
					<height>33</height>
					<texture border="2">list-focus.png</texture>
					<include>ListScroll</include>
				</control>
				<control type="label">
					<posx>10</posx>
					<posy>0</posy>
					<width>236</width>
					<height>33</height>
					<font>h2D</font>
					<align>left</align>
					<aligny>center</aligny>
					<info>ListItem.Label</info>
				</control>
			</focusedlayout>
			
		</control>
	</control>

</controls>
</window>
