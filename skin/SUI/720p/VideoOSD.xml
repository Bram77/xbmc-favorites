<window id="2901">
	<defaultcontrol>4</defaultcontrol>
	<include>dialogfade</include>
	<controls>
		<control type="image">
			<posy>120</posy>
			<texture>video-options-background.png</texture>
			<visible>window.isactive(123) | window.isactive(124) | window.isactive(125)</visible>
			<animation effect="fade" time="200">VisibleChange</animation>
		</control>
		<control type="image">
			<description>background</description>
			<posx>0</posx>
			<posy>310</posy>
			<width>1280</width>
			<height>410</height>
			<texture>videoOSD.png</texture>
			<aspectratio>stretch</aspectratio>
		</control>
		<control type="image">
			<description>dashes</description>
			<posx>0</posx>
			<posy>673</posy>
			<width>1280</width>
			<height>22</height>
			<texture>dashes.png</texture>
			<aspectratio>stretch</aspectratio>
		</control>
		<control type="image">
			<description>dash</description>
			<posx>530</posx>
			<posy>673</posy>
			<width>34</width>
			<height>22</height>
			<texture>dash.png</texture>
			<aspectratio>keep</aspectratio>
			<visible>VideoPlayer.HasMenu</visible>
		</control>
		<control type="button" id="11">
			<posx>435</posx>
			<posy>663</posy>
			<height>40</height>
			<description>dvd</description>
			<width>100</width>
			<label>dvd menu</label>
			<align>center</align>
			<onclick>xbmc.playercontrol(showvideomenu)</onclick>
			<visible>VideoPlayer.HasMenu</visible>
			<onleft>9</onleft>
			<onright>2</onright>
		</control>
		<control type="grouplist" id="1">
			<posx>560</posx>
			<posy>658</posy>
			<width>930</width>
			<height>40</height>
			<itemgap>20</itemgap>
			<onleft>11</onleft>
			<onright>11</onright>
			<ondown>1</ondown>
			<orientation>horizontal</orientation>
				<control type="button" id="2">
					<description>skip backwards</description>
					<width>90</width>
					<label>skipbkwd</label>
					<align>center</align>
					<onclick>xbmc.playercontrol(previous)</onclick>
				</control>
				<control type="button" id="3">
					<width>65</width>
					<description>pause</description>
					<label>stop</label>	
					<align>center</align>
					<onclick>xbmc.playercontrol(stop)</onclick>
				</control>
				<control type="togglebutton" id="4">
					<description>play</description>
					<width>55</width>
					<label>play</label>
					<altlabel>pause</altlabel>
					<align>center</align>
					<onclick>xbmc.playercontrol(play)</onclick> 
					<textcolor>normal</textcolor>
					<focusedcolor>selected</focusedcolor>
     					<texturefocus>-</texturefocus>
     					<texturenofocus>-</texturenofocus>
     					<alttexturefocus>-</alttexturefocus>
     					<alttexturenofocus>-</alttexturenofocus>
					<textoffsety>15</textoffsety>
				</control>
				<control type="button" id="6">
					<width>95</width>
					<description>skip forward</description>
					<label>skipfwd</label>
					<align>center</align>
					<onclick>xbmc.playercontrol(forward)</onclick>
				</control>
				<control type="button" id="7">
					<width>60</width>
					<description>video settings</description>
					<label>video</label>
					<align>center</align>
					<onup>xbmc.activatewindow(123)</onup>
					<onclick>xbmc.activatewindow(123)</onclick>
				</control>
				<control type="button" id="8">
					<width>60</width>
					<description>forward</description>
					<label>audio</label>
					<align>center</align>
					<onup>xbmc.activatewindow(124)</onup>
					<onclick>xbmc.activatewindow(124)</onclick>
				</control>
				<control type="button" id="9">
					<description>bookmarks</description>
					<width>110</width>
					<label>bookmarks</label>
					<align>center</align>
					<onup>xbmc.activatewindow(125)</onup>
					<onclick>XBMC.ActivateWindow(125)</onclick>
				</control>	
		</control>
	<control type="group" id="101">
		<posy>145</posy>
		<control type="group">
			<posx>195</posx>
			<posy>547</posy>
		<control type="label" id="0">
			<label>paused</label>
			<align>center</align>
			<visible>player.paused</visible>
		</control>
		<control type="label" id="0">
			<label>ff2x</label>
			<align>center</align>
			<visible>player.forwarding2x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>ff4x</label>
			<visible>player.forwarding4x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>ff8x</label>
			<visible>player.forwarding8x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>ff16x</label>
			<visible>player.forwarding16x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>ff32x</label>
			<visible>player.forwarding32x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>rw2x</label>
			<visible>player.rewinding2x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>rw4x</label>
			<visible>player.rewinding4x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>rw8x</label>
			<visible>player.rewinding8x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>rw16x</label>
			<visible>player.rewinding16x</visible>
		</control>
		<control type="label" id="0">
			<align>center</align>
			<label>rw32x</label>
			<visible>player.rewinding32x</visible>
		</control>
	</control>
		<control type="progress" id="23">
			<description>progressbar</description>
			<posx>50</posx>
			<posy>532</posy>
			<width>300</width>
			<height>12</height>
			<info>player.progress</info>
			<visible>true</visible>
		</control>
		<control type="image" id="0">
			<description>Playing Video Art</description>
			<posx>50</posx>
			<posy>85</posy>
			<width>300</width>
			<height>400</height>
			<texture fallback="">$INFO[videoplayer.cover]</texture>
			<aspectratio aligny="bottom" align="left">keep</aspectratio>
			<visible>VideoPlayer.Content(episodes)</visible>
		</control>
		<control type="image" id="0">
			<description>Playing Video Art</description>
			<posx>50</posx>
			<posy>65</posy>
			<width>300</width>
			<height>446</height>
			<texture fallback="">$INFO[videoplayer.cover]</texture>
			<aspectratio aligny="bottom" align="left">keep</aspectratio>
			<visible>VideoPlayer.Content(movies)</visible>
		</control>
		<control type="slider" id="401">
			<description>seek_slider</description>
			<posx>50</posx>
			<posy>558</posy>
			<width>300</width>
			<height>12</height>
			<texturesliderbar>seekslider.png</texturesliderbar>
			<textureslidernib>-</textureslidernib>
			<textureslidernibfocus>-</textureslidernibfocus>
			<visible>true</visible>
			<onup>1</onup>
			<ondown>1</ondown>
			<onleft>1</onleft>
			<onright>1</onright>
		</control>
		<control type="label">
			<posx>350</posx>
			<posy>547</posy>
			<height>40</height>
			<description>duration</description>
			<textcolor>normal</textcolor>
			<width>75</width>
			<label>$INFO[VideoPlayer.Duration]</label>
			<align>right</align>
		</control>
		<control type="label">
			<posx>50</posx>
			<posy>547</posy>
			<height>40</height>
			<textcolor>normal</textcolor>
			<description>Time</description>
			<width>75</width>
			<label>$INFO[VideoPlayer.Time]</label>
			<align>left</align>
		</control>
		<control type="label" id="0">
			<description>movie info</description>
			<width>400</width>
			<posx>50</posx>
			<posy>512</posy>
			<font>info</font>
			<visible>VideoPlayer.Content(movies)</visible>
			<textcolor>normal</textcolor>
			<align>left</align>
			<label>[LOWERCASE]$INFO[VideoPlayer.Title][/LOWERCASE]</label>
			<scroll>false</scroll>
		</control>
		<control type="label" id="0">
			<description>episode_info</description>
			<width>400</width>
			<posx>50</posx>
			<posy>488</posy>
			<font>info</font>
			<visible>VideoPlayer.Content(episodes)</visible>
			<textcolor>selected</textcolor>
			<align>left</align>
			<label>[LOWERCASE]$INFO[VideoPlayer.TVShowTitle] s[B]$INFO[VideoPlayer.Season][/B]e[B]$INFO[VideoPlayer.Episode][/B][CR]$INFO[VideoPlayer.Title][/LOWERCASE]</label>
			<scroll>false</scroll>
		</control>
	</control>
	</controls>
</window>