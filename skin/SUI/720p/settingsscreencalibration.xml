			<texturenofocus>-</texturenofocus>
		</control>
		<control type="resize" id="11">
			<description>pixel_aspect_ratio_box</description>
			<posx>0</posx>
			<posy>270</posy>
			<width>288</width>
			<height>288</height>
			<texturefocus>calibrate_pixelratio.png</texturefocus>
			<texturenofocus>-</texturenofocus>
		</control>
		<control type="image" id="1">
			<description>rectangle_image</description>
