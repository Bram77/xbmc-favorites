			<posy>256</posy>
			<width>720</width>
			<height>57</height>
			<texture>bluebar.png</texture>
		</control>
		<control type="label" id="2">
			<description>coordinates_label</description>
			<posx>55</posx>
			<posy>260</posy>
			<label>-</label>
			<textcolor>normal</textcolor>
		</control>
		<control type="label" id="3">
			<description>help_information</description>
			<posx>55</posx>
			<posy>280</posy>
			<label>-</label>
			<textcolor>normal</textcolor>
