			<include>file_name</include>
			<include>2041</include>
		</control>
		<control type="videowindow" id="20">
			<description>videowindow</description>
			<posx>0</posx>
			<posy>0</posy>
			<width>720</width>
			<height>576</height>
		</control>
		<control type="mover" id="8">
			<description>top_left_mover</description>
			<posx>0</posx>
			<posy>0</posy>
			<texturefocus>calibrate_topleft.png</texturefocus>
			<texturenofocus>-</texturenofocus>
		</control>
		<control type="mover" id="9">
			<description>right_bottom_mover</description>
			<posx>700</posx>
			<posy>500</posy>
