			</control>
			<control type="label">
				<description>track</description>
				<posy>110</posy>
				<width>240</width>
				<label>$LOCALIZE[435] :</label>
			</control>
		</control>
		<control type="grouplist" id="1">
				<posx>60</posx>
				<posy>200</posy>
				<width>400</width>
				<height>25</height>
				<itemgap>5</itemgap>
				<orientation>horizontal</orientation>
			<control type="button" id ="10">
				<description>yes</description>
				<label>186</label>
			</control>
			<control type="button" id="11">
				<description>no</description>
				<label>222</label>
			</control>
			<control type="button" id ="13">
				<description>get_thumb</description>
				<label>13405</label>
			</control>
		</control>
	</controls>
</window>