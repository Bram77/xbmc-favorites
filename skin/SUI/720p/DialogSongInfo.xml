			<height>150</height>
			<aspectratio aligny="bottom">keep</aspectratio>
			<info>listitem.icon</info>
		</control>
		<control type="group">
			<posx>160</posx>
			<control type="label">
				<description>song</description>
				<posy>10</posy>
				<width>240</width>
