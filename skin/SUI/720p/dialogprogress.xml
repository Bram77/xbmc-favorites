<window id="101">
	<defaultcontrol>10</defaultcontrol>
	<controls>
		<control type="image">
			<posx>0</posx>
			<posy>0</posy>
			<width>1280</width>
			<height>430</height>
			<texture flipx="true" flipy="true">videoosdcorner.png</texture>
			<colordiffuse>white</colordiffuse>
			<animation effect="fade" time="200">windowopen</animation>
			<animation effect="fade" time="200">windowclose</animation>
		</control>
		<control type="group">
			<posx>930</posx>
			<posy>25</posy>
			<control type="progress" id="20">
				<description>progress_control</description>
				<posy>25</posy>
				<width>261</width>
				<height>13</height>
			</control>
			<control type="group">
			<posx>260</posx>
			<animation effect="fade" time="250">windowopen</animation>
			<animation effect="fade" time="250">windowclose</animation>
			<control type="label" id="1">
				<description>heading</description>
				<label>label</label>
				<font>title</font>
				<textcolor>white</textcolor>
				<shadowcolor>selected</shadowcolor>
				<align>right</align>
			</control>
			<control type="label" id="2">
				<description>line_1</description>
				<posy>40</posy>
				<haspath>yes</haspath>
				<label></label>
				<align>right</align>
			</control>
			<control type="label" id="3">
				<description>line_2</description>
				<posy>65</posy>
				<haspath>yes</haspath>
				<label></label>
				<align>right</align>
			</control>
			<control type="label" id="4">
				<description>line_3</description>
				<posy>90</posy>
				<haspath>yes</haspath>
				<label></label>
				<align>right</align>
			</control>
			</control>
			<control type="button" id="10">
				<description>cancel</description>
				<posy>100</posy>
				<posx>20</posx>
				<label>cancel</label>
				<align>right</align>
			</control>
		</control>
	</controls>
</window>