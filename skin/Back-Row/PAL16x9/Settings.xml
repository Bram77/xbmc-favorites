<window>
	<id>4</id>
	<defaultcontrol>300</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<controls>

<control>
		<description>Panel Image</description>
		<type>image</type>
		<posx>0</posx>
		<posy>0</posy>
		<width>720</width>
		<height>576</height>
		<texture>background2.png</texture>
		<visible>!Skin.HasSetting(Background)</visible>
		</control>




<control type="multiimage">
		<posx>0</posx>
		<posy>0</posy>
		<width>720</width>
		<height>576</height>
		<imagepath>Menus</imagepath>
		<info>Skin.String(CustomMenu)</info>
		<timeperimage>5000</timeperimage>
		<fadetime>2000</fadetime>
        <randomize>true</randomize>
		<visible>!Skin.HasSetting(CustomMenu)</visible>
		<visible>Skin.HasSetting(Background)</visible>
		<animation effect="fade" time="200">WindowOpen</animation>
        <animation effect="fade" time="300">WindowClose</animation>
        <animation effect="fade" time="200">Visible</animation>
        <animation effect="fade" delay = "200" time="800">Hidden</animation>
		</control>




		<control type="multiimage">
		<posx>0</posx>
		<posy>0</posy>
		<width>720</width>
		<height>576</height>
		<imagepath>-</imagepath>
		<info>Skin.String(CustomMenuFolder)</info>
		<timeperimage>5000</timeperimage>
		<fadetime>2000</fadetime>
        <randomize>true</randomize>
		<visible>!Skin.HasSetting(CustomMenuFolder)</visible>
		<visible>Skin.HasSetting(Background)</visible>
		<animation effect="fade" time="200">WindowOpen</animation>
        <animation effect="fade" time="300">WindowClose</animation>
        <animation effect="fade" time="200">Visible</animation>
        <animation effect="fade" delay = "200" time="800">Hidden</animation>
		</control>

<control>
		<description>Background</description>
		<type>image</type>
		<posx>0</posx>
		<posy>0</posy>
		<width>720</width>
		<height>576</height>
		<texture>Back-Row.png</texture>
		<animation effect="fade" time="200">WindowOpen</animation>
	    <animation effect="fade" time="200">WindowClose</animation>
		<visible>!Skin.HasSetting(Background)</visible>
        </control>

<control type="image">
    <description>Logo</description>
    <posx>15</posx>
    <posy>10</posy>
    <width>30</width>
    <height>40</height>
    <texture>icon-settings.png</texture>
    <visible>!Skin.HasSetting(menulabels)</visible>
	<animation effect="fade" time="200">WindowOpen</animation>
	<animation effect="fade" time="200">WindowClose</animation>
	 </control>




<control>
		<description>Description label</description>
		<label>5</label>
		<font>font18</font>
	<textcolor>white</textcolor>
		<include>description</include>
	</control>
	

<control>
		<description>Background</description>
		<type>image</type>
		<posx>-10</posx>
		<posy>130</posy>
		<width>740</width>
		<height>320</height>
		<texture>film1.png</texture>
		<animation effect="fade" time="200">WindowOpen</animation>
	    <animation effect="fade" time="200">WindowClose</animation> 
		<visible>!Skin.HasSetting(Background)</visible>
