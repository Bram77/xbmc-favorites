	<!-- Settings ButtonScroller -->
	<controlgroup>
	<animation effect="fade" time="200">WindowOpen</animation>
	<animation effect="fade" time="200">WindowClose</animation> 
	 <posy>-20</posy>
	
	<control>
        <description>Home Buttons</description>
        <type>buttonscroller</type>
        <id>300</id>
        <posx>-420</posx>
        <posy>205</posy>
        <width>137</width>
        <height>167</height>
        <buttongap>5</buttongap>
        <numbuttons>15</numbuttons>
        <orientation>horizontal</orientation>
        <align>center</align>
        <aligny>center</aligny>
        <movement>0</movement>
        <defaultbutton>6</defaultbutton>
        <wraparound>yes</wraparound>
        <smoothscrolling>yes</smoothscrolling>
        <alpha>80</alpha>
        <onup>300</onup>
        <ondown>300</ondown>
        <visible>!Skin.HasSetting(Settings-menu)</visible>
        <animation effect="slide" start="720,0" end="0,0" acceleration="-1.0" time="200">WindowOpen</animation>
        <buttons>
        <button id="1">
		<description>Appearance</description>
		<label></label>
		<onclick>XBMC.ActivateWindow(19)</onclick>
