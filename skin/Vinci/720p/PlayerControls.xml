<window>
  <id>114</id>
  <defaultcontrol>403</defaultcontrol>
  <visible>Player.HasMedia + Window.IsActive(PlayerControls) + !Window.IsActive(FullscreenVideo)</visible>
  <include>dialog-effect</include>
  <type>dialog</type>
  <coordinates>
    <system>1</system>
    <posx>162</posx>
    <posy>657</posy>
  </coordinates>
  <controls>

    <control>
      <type>button</type>
      <id>400</id>
      <posx>0</posx>
      <posy>0</posy>
      <width>26</width>
      <height>24</height>
      <label>-</label>
      <texturefocus>music-skipminus-focus.png</texturefocus>
      <texturenofocus>music-skipminus-nofocus.png</texturenofocus>
      <onleft>405</onleft>
      <onright>401</onright>
      <onup>400</onup>
      <ondown>400</ondown>
      <onclick>XBMC.PlayerControl(Previous)</onclick>
    </control>

    <control>
      <type>button</type>
      <id>401</id>
      <posx>26</posx>
      <posy>0</posy>
      <width>26</width>
      <height>24</height>
      <label>-</label>
      <texturefocus>music-rw-focus.png</texturefocus>
      <texturenofocus>music-rw-nofocus.png</texturenofocus>
      <onleft>400</onleft>
      <onright>402</onright>
      <onup>401</onup>
      <ondown>401</ondown>
      <onclick>XBMC.PlayerControl(Rewind)</onclick>
    </control>

    <control>
      <type>button</type>
      <id>402</id>
      <posx>52</posx>
      <posy>0</posy>
      <width>26</width>
      <height>24</height>
      <label>-</label>
      <texturefocus>music-stop-focus.png</texturefocus>
      <texturenofocus>music-stop-nofocus.png</texturenofocus>
      <onleft>401</onleft>
      <onright>403</onright>
      <onup>402</onup>
      <ondown>402</ondown>
      <onclick>XBMC.PlayerControl(Stop)</onclick>
    </control>

    <control>
      <type>togglebutton</type>
      <id>403</id>
      <posx>78</posx>
      <posy>0</posy>
      <width>26</width>
      <height>24</height>
      <label>-</label>
      <texturefocus>music-pause-focus.png</texturefocus>
      <texturenofocus>music-pause-nofocus.png</texturenofocus>
      <usealttexture>Player.Paused</usealttexture>
      <alttexturefocus>music-play-focus.png</alttexturefocus>
      <alttextureNofocus>music-play-nofocus.png</alttextureNofocus>
      <onleft>402</onleft>
      <onright>404</onright>
      <onup>403</onup>
      <ondown>403</ondown>
      <onclick>XBMC.PlayerControl(Play)</onclick>
    </control>

    <control>
      <type>button</type>
      <id>404</id>
      <posx>104</posx>
      <posy>1</posy>
      <width>26</width>
      <height>24</height>
      <label>-</label>
      <texturefocus>music-ff-focus.png</texturefocus>
      <texturenofocus>music-ff-nofocus.png</texturenofocus>
      <onleft>403</onleft>
      <onright>405</onright>
      <onup>404</onup>
      <ondown>404</ondown>
      <onclick>XBMC.PlayerControl(Forward)</onclick>
    </control>

    <control>
      <type>button</type>
      <id>405</id>
      <posx>130</posx>
      <posy>0</posy>
      <width>26</width>
      <height>24</height>
      <label>-</label>
      <texturefocus>music-skipplus-focus.png</texturefocus>
      <texturenofocus>music-skipplus-nofocus.png</texturenofocus>
      <onleft>404</onleft>
      <onright>400</onright>
      <onup>405</onup>
      <ondown>405</ondown>
      <onclick>XBMC.PlayerControl(Next)</onclick>
    </control>

  </controls>

</window>

