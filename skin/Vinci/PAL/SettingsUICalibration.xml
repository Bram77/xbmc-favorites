<window>
  <id>10</id>
  <defaultcontrol>2</defaultcontrol>
  <allowoverlay>yes</allowoverlay>
  <controls>

    <control>
      <description>background image</description>
      <type>image</type>
      <id>1</id>
      <posx>0</posx>
      <posy>0</posy>
      <width>720</width>
      <height>576</height>
      <texture>background-UIcalibration.png</texture>
    </control>

    <control>
      <description>rectangle image</description>
      <type>mover</type>
      <id>2</id>
      <posx>0</posx>
      <posy>271</posy>
      <width>720</width>
      <height>40</height>
      <texturefocus>bluebar.png</texturefocus>
      <texturenofocus>bluebar.png</texturenofocus>
    </control>

    <control>
      <description>coordinates label</description>
      <type>label</type>
      <id>3</id>
      <posx>360</posx>
      <posy>275</posy>
      <label>5</label>
      <font>font04</font>
    </control>

  </controls>

</window>

