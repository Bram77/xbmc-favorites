<window>
  <id>3</id>
  <defaultcontrol>20</defaultcontrol>
  <allowoverlay>no</allowoverlay>
  <controls>
    <include>background</include>

    <control>
      <description>filemanager background image 1</description>
      <type>image</type>
      <width>720</width>
      <height>576</height>
      <texture>filemanager_bg.png</texture>
      <visible>![Skin.HasSetting(visualisation) + Player.HasMedia]</visible>
    </control>

    <control>
      <description>filemanager background image 2</description>
      <type>image</type>
      <width>720</width>
      <height>576</height>
      <texture>trainerpanel_bg.png</texture>
      <visible>[Skin.HasSetting(visualisation) + Player.HasMedia]</visible>
    </control>

    <control>
      <description>current Directory text label left</description>
      <type>fadelabel</type>
      <id>101</id>
      <align>left</align>
      <posx>90</posx>
      <posy>65</posy>
      <width>270</width>
      <font>font13</font>
      <textcolor>D0FFFFFF</textcolor>
    </control>

    <control>
      <description>current Directory text label right</description>
      <type>fadelabel</type>
      <id>102</id>
      <align>left</align>
      <posx>380</posx>
      <posy>65</posy>
      <width>270</width>
      <font>font13</font>
      <textcolor>D0FFFFFF</textcolor>
    </control>

    <control>
      <description>number of files in left list text label</description>
      <type>label</type>
      <id>12</id>
      <posx>90</posx>
      <posy>484</posy>
      <label>-</label>
      <font>font13</font>
      <textcolor>D0FFFFFF</textcolor>
    </control>

    <control>
      <description>number of files in right list text label</description>
      <type>label</type>
      <id>13</id>
      <posx>380</posx>
      <posy>484</posy>
      <label>-</label>
      <font>font13</font>
      <textcolor>D0FFFFFF</textcolor>
    </control>

    <control>
      <description>Left File list</description>
      <type>listcontrol</type>
      <id>20</id>
      <selectedcolor>FFFF9600</selectedcolor>
      <posx>74</posx>
      <posy>102</posy>
      <width>290</width>
      <height>400</height>
      <onleft>21</onleft>
      <onright>21</onright>
      <onup>20</onup>
      <ondown>20</ondown>
    </control>

    <control>
      <description>Right File list</description>
      <type>listcontrol</type>
      <id>21</id>
      <selectedcolor>FFFF9600</selectedcolor>
      <posx>364</posx>
      <posy>102</posy>
      <width>290</width>
      <height>400</height>
      <onleft>20</onleft>
      <onright>20</onright>
      <onup>21</onup>
      <ondown>21</ondown>
    </control>

    <control>
      <description>filemanager background image</description>
      <type>image</type>
      <id>1</id>
      <width>720</width>
      <height>576</height>
      <texture>filemanager.png</texture>
    </control>

  </controls>

</window>