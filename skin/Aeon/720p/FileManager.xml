<window type="dialog" id="3">
	<defaultcontrol>20</defaultcontrol>
	<allowoverlay>yes</allowoverlay>
	<coordinates>
	<system>1</system>
	<posx>0</posx>
	<posy>0</posy>
	</coordinates>
	<controls>

	<control type="multiimage">
		<posx>0</posx>
		<posy>0</posy>
		<width>720</width>
		<height>576</height>
		<imagepath>Files</imagepath>
		<info>Skin.String(CustomFiles)</info>
		<visible>!Skin.HasSetting(CustomFiles) + !Skin.HasSetting(CustomFilesFoldOn)</visible>
	</control>

	<control type="multiimage">
		<posx>0</posx>
		<posy>0</posy>
		<width>720</width>
		<height>576</height>
		<imagepath>-</imagepath>
		<timeperimage>20000</timeperimage>
		<fadetime>2000</fadetime>
		<randomize>true</randomize>
		<info>Skin.String(CustomFilesFolder)</info>
		<visible>!Skin.HasSetting(CustomFilesFolder)</visible>
	</control>

	<control type="image">
		<posx>0</posx>
		<posy>55</posy>
		<width>720</width>
		<height>475</height>
		<texture>filemanback.png</texture>
		<include>common-fade-quick</include>
	</control>

