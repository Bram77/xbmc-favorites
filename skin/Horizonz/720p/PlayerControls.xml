		<posx>325</posx>
		<posy>12</posy>
		<width>30</width>
		<height>30</height>
		<label>-</label>
		<texturefocus>Media-Next-Focus.png</texturefocus>
		<texturenofocus>Media-Next-NoFocus.png</texturenofocus>
		<onleft>404</onleft>
		<onright>406</onright>
		<onup>405</onup>
		<ondown>405</ondown>
		<onclick>XBMC.PlayerControl(Next)</onclick>
	</control>

	<control type="button" id="406">
		<description>Repeat Button</description>
		<posx>357</posx>
		<posy>12</posy>
		<width>30</width>
		<height>30</height>
		<label>-</label>
		<texturefocus>-</texturefocus>
		<texturenofocus>-</texturenofocus>
		<onleft>405</onleft>
		<onright>398</onright>
		<onup>406</onup>
		<ondown>406</ondown>
		<onclick>XBMC.PlayerControl(Repeat)</onclick>
	</control>
	<control type="image">
		<posx>357</posx>
		<posy>12</posy>
		<width>30</width>
		<height>30</height>
		<texture>Media-RepeatA-NoFocus.png</texture>
		<visible>!Playlist.IsRepeat + !Playlist.IsRepeatOne</visible>
		<visible>!Control.HasFocus(406)</visible>
	</control>
	<control type="image">
		<posx>357</posx>
		<posy>12</posy>
		<width>30</width>
		<height>30</height>
		<texture>Media-RepeatA-Focus.png</texture>
		<visible>!Playlist.IsRepeat + !Playlist.IsRepeatOne</visible>
		<visible>Control.HasFocus(406)</visible>
	</control>
	<control type="image">
		<posx>357</posx>
		<posy>12</posy>
		<width>30</width>
		<height>30</height>
		<texture>Media-RepeatOne-NoFocus.png</texture>
