<window type="dialog" id="114">
	<defaultcontrol>402</defaultcontrol>
	<zorder>3</zorder>
	<coordinates>
	<system>2</system>
	<posx>870</posx>
	<posy>668</posy>
	</coordinates>
	<animation effect="slide" start="410,0" end="0,0" time="150" condition="!Window.IsActive(FullscreenVideo)">WindowOpen</animation>
		<animation effect="slide" start="0,0" end="410,0" time="150" condition="!Window.IsActive(FullscreenVideo)">WindowClose</animation>
	<visible>Player.HasMedia + Window.IsActive(PlayerControls) + !Window.IsActive(FullscreenVideo)</visible>
	<controls>

		<control type="image">
		<description>Background Left</description>
		<posx>0</posx>
		<posy>0</posy>
		<width>410</width>
		<height>52</height>
		<texture>playerpanel.png</texture>
	</control>

	<control type="button" id="398">
		<description>Media Button</description>
		<posx>101</posx>
		<posy>12</posy>
		<width>30</width>
		<height>30</height>
		<label>-</label>
		<texturefocus>Media-Info-Focus.png</texturefocus>
		<texturenofocus>Media-Info-NoFocus.png</texturenofocus>
		<onleft>406</onleft>
		<onright>399</onright>
		<onup>398</onup>
		<ondown>398</ondown>
		<onclick>Skin.ToggleSetting(MediaInfo)</onclick>
	</control>

	<control type="togglebutton" id="399">
		<description>Mute Button</description>
		<posx>133</posx>
		<posy>12</posy>
		<width>30</width>
		<height>30</height>
		<label>-</label>
		<texturefocus>Media-Mute-Off-Focus.png</texturefocus>
		<texturenofocus>Media-Mute-Off-NoFocus.png</texturenofocus>
		<usealttexture>Player.Muted</usealttexture>
		<alttexturefocus>Media-Mute-On-Focus.png</alttexturefocus>
		<alttexturenofocus>Media-Mute-On-NoFocus.png</alttexturenofocus>
		<onleft>398</onleft>
		<onright>400</onright>
		<onup>399</onup>
		<ondown>399</ondown>
		<onclick>XBMC.Mute</onclick>
	</control>

	<control type="button" id="400">
		<description>Previous Button</description>
		<posx>162</posx>
		<posy>12</posy>
		<width>30</width>
		<height>30</height>
		<label>-</label>
		<texturefocus>Media-Previous-Focus.png</texturefocus>
		<texturenofocus>Media-Previous-NoFocus.png</texturenofocus>
		<onleft>399</onleft>
		<onright>401</onright>
		<onup>400</onup>
		<ondown>400</ondown>
		<onclick>XBMC.PlayerControl(Previous)</onclick>
	</control>

	<control type="button" id="401">
		<description>Rewind Button</description>
		<posx>197</posx>
		<posy>12</posy>
		<width>30</width>
		<height>30</height>
		<label>-</label>
		<texturefocus>Media-Rewind-Focus.png</texturefocus>
		<texturenofocus>Media-Rewind-NoFocus.png</texturenofocus>
		<onleft>400</onleft>
		<onright>402</onright>
		<onup>401</onup>
		<ondown>401</ondown>
		<onclick>XBMC.PlayerControl(Rewind)</onclick>
	</control>

	<control type="button" id="402">
		<description>Stop Button</description>
		<posx>229</posx>
		<posy>12</posy>
