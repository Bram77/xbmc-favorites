<window>
	<id>2</id>
	<defaultcontrol>50</defaultcontrol>
	<animation effect="fade" time="800">WindowClose</animation> 
	<allowoverlay>yes</allowoverlay>
	<controls>

<control>
		<description>Panel Image</description>
		<type>image</type>
		<posx>5</posx>
		<posy>10</posy>
		<width>710</width>
		<height>556</height>
		<texture>background2.png</texture>
        </control>



<control>
		<description>Background</description>
		<type>image</type>
		<posx>0</posx>
		<posy>0</posy>
		<width>720</width>
		<height>576</height>
		<texture>blacknwhite.png</texture>
		<animation effect="fade" time="800">WindowOpen</animation>
	    <animation effect="fade" time="800">WindowClose</animation> 
        </control>
	


<control>
		<description>Panel Image</description>
		<type>image</type>
		<posx>0</posx>
		<posy>0</posy>
		<width>750</width>
		<height>576</height>
		<texture>photobg.png</texture>
		<visible>!Skin.HasSetting(Wallpaper)</visible>
		<visible>!Skin.HasSetting(Default Wallpaper)</visible>

		</control>

		<control type="multiimage">
		<posx>0</posx>
		<posy>0</posy>
		<width>720</width>
		<height>576</height>
		<imagepath>Photos</imagepath>
		<info>Skin.String(CustomPictures)</info>
		<timeperimage>5000</timeperimage>
		<fadetime>2000</fadetime>
        <randomize>true</randomize>
		<visible>Skin.HasSetting(Wallpaper)</visible>
		<visible>!Skin.HasSetting(Default Wallpaper) + !Skin.HasSetting(CustomPictures) + !Skin.HasSetting(CustomMusicFoldOn)</visible>


		<animation effect="fade" time="400">WindowOpen</animation>
         <animation effect="fade" time="300">WindowClose</animation>
         <animation effect="fade" time="800">Visible</animation>
         <animation effect="fade" delay = "200" time="800">Hidden</animation>
		</control>
	
	

<include>listcontrol</include>

<include>List 2</include>

<include>List 3</include>	

<include>CD Wrap</include>
	
<include>Bigwraplist</include>	

<include>Wallpaper</include>
 
<include>Square Small Icons</include>	

<control type="image">
    <description>list item icon</description>
    <posx>20</posx>
    <posy>100</posy>
    <width>350</width>
    <height>350</height>
    <info>ListItem.Icon</info>
    <aspectratio>keep</aspectratio>
    <visible>Control.IsVisible(50)</visible>
    </control>

    <control type="image">
    <description>list item icon reflection</description>
    <posx>20</posx>
    <posy>450</posy>
    <width>350</width>
    <height>350</height>
    <info>ListItem.Icon</info>
    <texture flipy="true" diffuse="Thumb_Mask.png" />
    <aspectratio>keep</aspectratio>
    <visible>Control.IsVisible(50)</visible>
    </control>

	
	<control type="image">
    <description>Logo</description>
    <posx>15</posx>
    <posy>10</posy>
    <width>30</width>
    <height>40</height>
    <texture>MyPictures.png</texture>
     <aspectratio>keep</aspectratio>

    <visible>!Skin.HasSetting(menulabels)</visible>
	 <animation effect="fade" time="200">WindowOpen</animation>
	<animation effect="fade" time="200">WindowClose</animation>
	 </control>


	<control>
		<description>Description label</description>
		<label>1</label>
		<font>font18</font>
	<textcolor>FFffffff</textcolor>
		<include>description</include>
	</control>



