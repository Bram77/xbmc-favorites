<window type="dialog" id="29">
	
	<defaultcontrol always="true">52</defaultcontrol>
	<allowoverlay>no</allowoverlay>
	<coordinates>
	<system>1</system>
	<posx>0</posx>
	<posy>0</posy>
	</coordinates>
	<controls>
	
	
	<control>
		<description>Panel Image</description>
		<type>image</type>
		<posx>0</posx>
		<posy>0</posy>
		<width>720</width>
		<height>556</height>
		<texture>background2.png</texture>
        </control>



<control>
		<description>Background</description>
		<type>image</type>
		<posx>0</posx>
		<posy>0</posy>
		<width>720</width>
		<height>388</height>
		<texture>Back-Row.png</texture>
		<animation effect="fade" time="800">WindowOpen</animation>
	    
        </control>

<control>
		<description>Panel Image</description>
		<type>image</type>
		<posx>0</posx>
		<posy>388</posy>
		<width>720</width>
		<height>216</height>
		<texture>background2.png</texture>
        <animation effect="fade" time="800">WindowOpen</animation>
	    
        </control>

		<control>
		<description>Panel Image</description>
		<type>image</type>
		<posx>0</posx>
		<posy>358</posy>
		<width>720</width>
		<height>216</height>
		<texture>chairs1.png</texture>
        <animation effect="fade" time="800">WindowOpen</animation>
	    
        </control>



	
	<control type="wraplist" id="52">
    <viewtype label="Big WrapList">bigwrap</viewtype>
     <posx>2.5</posx>
     <posy>67</posy>
     <width>720</width>
     <height>504</height>
     <onleft>90</onleft>
     <onright>90</onright>
     <onup>2</onup>
     <ondown>98</ondown>
     <focusposition>1</focusposition>
     <orientation>horizontal</orientation>
     <itemlayout height="270" width="240">
      
	  <control type="image">
       <posx>10</posx>
       <posy>90</posy>
       <width>220</width>
       <height>270</height>
       <info>ListItem.Icon</info>
       <aspectratio aligny="bottom">keep</aspectratio>
      </control>
     
      
          
     </itemlayout>
     
	 <focusedlayout height="270" width="240">
      <control type="image">
       <posx>10</posx>
       <posy>90</posy>
       <width>220</width>
       <height>270</height>
       <info>ListItem.Icon</info>
       <aspectratio aligny="bottom">keep</aspectratio>
       </control>
      
            
     </focusedlayout>
   </control>
   <control type="label">
    <posx>30</posx>
    <posy>500</posy>
    <width>660</width>
    <height>16</height>
    <font>special14</font>
    <textcolor>FFffffff</textcolor>
    <align>center</align>
    <info>ListItem.Label</info>
    <include>selectedcolor</include>
    <visible>Control.IsVisible(52)</visible>
    </control>
    

	<!-- Logon Info -->
	<control type="label">
		<posx>100</posx>
		<posy>530</posy>
		<width>520</width>
		<height>16</height>
		<font>special12</font>
		<align>center</align>
		<textcolor>FFffffff</textcolor>
		<info>ListItem.Label2</info>
		<textcolor>FFffffff</textcolor>
		<visible>Control.IsVisible(52)</visible>
	</control>



	<control type="label" id="3">
		<description>Profile Count Label</description>
		<posx>40</posx>
		<posy>528</posy>
		<width>420</width>
		<!--<align>-</align>-->
		<font>special13</font>
		<textcolor>FFffffff</textcolor>
		<shadowcolor>DD000000</shadowcolor>
		<visible>!Window.IsActive(130) + !Window.IsActive(131)</visible>
	</control>

	<control>
		<description>Submenu Button</description>
		<type>button</type>
		<id>98</id>
		<posx>660</posx>
		<posy>520</posy>
		<width>40</width>
		<height>40</height>
		<onclick>ActivateWindow(ShutdownMenu)</onclick>
		<texturefocus>home-power-focus.png</texturefocus>
		<texturenofocus>-</texturenofocus>
		<label>-</label>
		<onleft>52</onleft>
		<onright>52</onright>
		<onup>52</onup>
		<ondown>52</ondown>
	</control>
	


<!-- Date and Time Labels -->
